# TradeVision AI - Environment Variables
# Copy this file to .env.local and fill in your values

# ===========================================
# API KEYS (Required for full functionality)
# ===========================================

# Google Gemini API Key for AI features
# Get your key at: https://makersuite.google.com/app/apikey
VITE_GEMINI_API_KEY=

# ===========================================
# FEATURE FLAGS (Optional)
# ===========================================

# Enable/disable AI features (default: true if API key is set)
VITE_ENABLE_AI_FEATURES=true

# Enable analytics tracking (default: false)
VITE_ENABLE_ANALYTICS=false

# ===========================================
# API ENDPOINTS (Optional - defaults provided)
# ===========================================

# Binance API base URL
# VITE_BINANCE_API_URL=https://api.binance.com

# Binance WebSocket URL
# VITE_BINANCE_WS_URL=wss://stream.binance.com:9443

# ===========================================
# APP CONFIG (Optional)
# ===========================================

# Application name
VITE_APP_NAME=TradeVision AI

# Application version
VITE_APP_VERSION=0.0.1
